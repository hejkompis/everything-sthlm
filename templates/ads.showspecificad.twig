<div class="container">
	<div class="row">
		<div class="col-xs-12">
				<div class="adBox">
					<h2>{{ad.title}}</h2>
					{% if ad.tags %}

						{% for tag in tags %}
							{% if tag.id in ad.tags %}<h4>{{tag.name}}{% endif %}</h4>
						{% endfor %}

					{% endif %}
					<p>{{ad.content}}</p>
					<p>Publicerad {{ad.dateCreated}}</p>
					<p>Typ av annons {{ad.type}}</p>
				</div>
		</div>
	</div>
</div>