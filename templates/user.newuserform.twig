<div class="container">
	<div class="row">
	<div class="col-xs-12">
		<form action="/user/savenewuser/" method="post">
			<div class="form-group">
				<label for="firstname">Förnamn</label>
				<input type="text" id="firstname" name="firstname" class="form-control" placeholder="John">
			</div>
			<div class="form-group">
				<label for="lastname">Efternamn</label>
				<input type="text" id="lastname" name="lastname" class="form-control" placeholder="Doe">
			</div>
			<div class="form-group">
				<label for="address_street">Gatuadress</label>
				<input type="text" id="address_street" name="address_street" class="form-control" placeholder="Adressgatan 1">
			</div>
			<div class="form-group">
				<label for="address_zip">Postnummer</label>
				<input type="text" id="address_zip" name="address_zip" class="form-control">
			</div>
			<div class="form-group">
				<label for="address_city">Ort</label>
				<input type="text" id="address_city" name="address_city" class="form-control">
			</div>
			<div class="form-group">
				<label for="email">E-post</label>
				<input type="text" id="email" name="email" class="form-control">
			</div>
			<div class="form-group">
				<label for="password">Lösenord</label>
				<input type="password" id="password" name="password" class="form-control">
			</div>
			<div class="form-group">
				<label for="repeatedpassword">Upprepa Lösenord</label>
				<input type="password" id="repeatedpassword" name="repeatedpassword" class="form-control">
			</div>
			<div class="form-group">
				<button type="submit" class="btn btn-primary">Skapa användare</button>
			</div>
		</form>
		
	</div>
	</div>
</div>